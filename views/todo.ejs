<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Todo List</title>
    </head>
    <body>
        <h1>Todo List</h1>
        <h2>Completed Tasks:</h2>
        <ul>
            <% comptasks.forEach(task => { %>
            <li>
                <strong>ID:</strong> <%= task.Id %>, <strong>Task:</strong> <%=
                task.Task %>, <strong>Created:</strong> <%= task.Created %>
            </li>
            <% }); %>
        </ul>
        <h2>Incompleted Tasks:</h2>
        <ul>
            <% noncomptasks.forEach(task => { %>
            <li>
                <strong>ID:</strong> <%= task.Id %>, <strong>Task:</strong> <%=
                task.Task %>, <strong>Created:</strong> <%= task.Created %>
            </li>
            <% }); %>
        </ul>
    </body>
</html>
